ontent\"\n            value={content}\n            placeholder={`${auth.user.username}, article likhna hai? ðŸ’«`}\n            onChange={(e) => setContent(e.target.value)}\n            style={{\n              filter: theme ? \"invert(1)\" : \"invert(0)\",\n              color: theme ? \"white\" : \"#111\",\n              background: theme ? \"rgba(0,0,0,.03)\" : \"\",\n            }}\n          />\n          <input \n            type={\"text\"}\n            name=\"motive\"\n            value={motive}\n            placeholder={`${auth.user.username}, motive? ðŸ’«`}\n            onChange={(e) => setMotive(e.target.value)}\n            style={{\n              filter: theme ? \"invert(1)\" : \"invert(0)\",\n              color: theme ? \"white\" : \"#111\",\n              background: theme ? \"rgba(0,0,0,.03)\" : \"\",\n            }}\n          />\n\n          <div className=\"d-flex\">\n            <div className=\"flex-fill\"></div>\n            <Icons setContent={setContent} content={content} theme={theme} />\n          </div>\n\n        </div>\n\n        <div className=\"status_footer\">\n          <button className=\"btn btn-primary w-100\" type=\"submit\">\n            Post\n          </button>\n        </div>\n      </form>\n    </div>\n  );\n};\n\nexport default ArticleModal;\n","/home/karishma/Documents/projects/Mo-react/client/src/redux/reducers/articleStatusReducer.js",[],"/home/karishma/Documents/projects/Mo-react/client/src/components/home/Article.js",["421","422","423","424","425","426"],"/home/karishma/Documents/projects/Mo-react/client/src/components/home/post_card/ArticleBody.js",["427"],"/home/karishma/Documents/projects/Mo-react/client/src/components/ArticleCard.js",["428","429","430"],{"ruleId":"431","replacedBy":"432"},{"ruleId":"433","replacedBy":"434"},{"ruleId":"435","severity":1,"message":"436","line":10,"column":8,"nodeType":"437","messageId":"438","endLine":10,"endColumn":15},{"ruleId":"435","severity":1,"message":"439","line":5,"column":8,"